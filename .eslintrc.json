{
    "env": {
      "commonjs": true,
      "es6": true,
      "node": true,
      "jest": true
    },
    "extends": ["airbnb-base"],
    "rules": {
      "no-param-reassign": "off",
      "class-methods-use-this": "off",
      "no-undef": "off",
      "implicit-arrow-linebreak": "off",
      "arrow-parens": "off"
    },
    "globals": {
      "Atomics": "readonly",
      "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
      "ecmaVersion": 2018
    }
  }
  